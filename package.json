{
  "name": "basenvsync",
  "displayName": "BASEnvSync",
  "description": "Utility to export/import SAP Business Application Studio dev space settings and configuration",
  "icon": "doc/logo.png",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualiture/BASEnvSync.git"
  },
  "keywords": [
    "SAP",
    "BAS",
    "Business Application Studio",
    "BTP",
    "Business Technology Platform"
  ],
  "author": "Robin van het Hof (Qualiture)",
  "publisher": "Qualiture",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qualiture/BASEnvSync/issues"
  },
  "homepage": "https://github.com/qualiture/BASEnvSync#readme",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "basenvsync.export",
        "title": "Export BAS environment settings"
      },
      {
        "command": "basenvsync.import",
        "title": "Import BAS environment settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run copy-webviews",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "copy-webviews": "cp -r ./src/webviews ./out/webviews"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.33.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "dependencies": {
    "fs-extra": "^11.3.1",
    "jszip": "^3.10.1"
  }
}
